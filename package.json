{
    "name": "line-jumper",
    "displayName": "line-jumper",
    "description": "extenstion to jump multiple lines at a time",
    "version": "0.0.1",
    "publisher": "alekseychaikovsky",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.moveUp",
        "onCommand:extension.moveDown",
        "onCommand:extension.selectUp",
        "onCommand:extension.selectDown"
    ],
    "main": "./extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Line-Jumper Configuration",
            "properties": {
                "lineJumper.linesToJump": {
                    "type": "integer",
                    "default": 10,
                    "description": "Number of lines to jump"
                }
            }
        },
        "commands": [{
            "command": "extension.moveUp",
            "title": "Move Up"
        },
        {
            "command": "extension.moveDown",
            "title": "Move Down"
        },
        {
            "command": "extension.selectUp",
            "title": "Select Up"
        },
        {
            "command": "extension.selectDown",
            "title": "Select Down"
        }],
        "keybindings": [{
            "command": "extension.moveUp",
            "key": "alt+up",
            "mac": "alt+up",
            "when": "editorTextFocus"
        },
        {
            "command": "extension.moveDown",
            "key": "alt+down",
            "mac": "alt+down",
            "when": "editorTextFocus"
        },
        {
            "command": "extension.selectUp",
            "key": "shift+alt+up",
            "mac": "shift+alt+up",
            "when": "editorTextFocus"
        },
        {
            "command": "extension.selectDown",
            "key": "shift+alt+down",
            "mac": "shift+alt+down",
            "when": "editorTextFocus"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "vscode": "^0.11.0"
    }
}